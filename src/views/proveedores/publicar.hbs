{{>navigation2}}
{{>message}} 

<div class="main">
    <!--====== CONTACT ONE PART START ======-->
<section class="contact-area">
   <div class="container">

      <div class="contact-form form-style-four mt-15" >
            <p class="h1">Publicar Solicitud</p>
               <form action="./publicar" method="POST" enctype="multipart/form-data">
                  <div class="row">
                     <div class="col-md-6">
                        <div class="form-input mt-15">
                           <label>Nombre del servicio</label>
                           <div class="input-items default">
                              <i class="lni lni-user"></i>
                              <input type="text" id="nombre" name="nombre" placeholder="Construccion de Casas" required/>
                           </div>
                        </div>
                        <!-- form input -->
                     </div>

                     <div class="col-md-6">
                        <div class="form-input mt-15">
                           <label>Marca de la Maquinaria</label>
                           <div class="input-items default">
                              <i class="lni lni-envelope"></i>
                              <input type="text" id="marca" name="marca" placeholder="Caterpillar" required />
                           </div>
                        </div>
                        <!-- form input -->
                     </div>
                     <div class="col-md-6">
                        <div class="form-input mt-15">
                           <label>Modelo</label>
                           <div class="input-items default">
                              <i class="lni lni-bubble"></i>
                              <input type="text"name="modelo" id="inputAddress2" placeholder="Compactadores" required />
                           </div>
                        </div>
                        <!-- form input -->
                     </div>

                     <div class="col-md-6">
                        <div class="form-input mt-15">
                           <label>Año máquina</label>
                           <div class="input-items default">
                              <i class="lni lni-envelope"></i>
                              <input type="number" id="ano" name="ano" placeholder="2011" required />
                           </div>
                        </div>
                        <!-- form input -->
                     </div>

                     

                     <div class="col-md-6">
                        <div class="form-input mt-15">
                           <label>Horometro</label>
                           <div class="input-items default">
                              <i class="lni lni-envelope"></i>
                              <input type="number" id="horometro" name="horometro" placeholder="En Horas ej: '3525'" required/>
                           </div>
                        </div>
                        <!-- form input -->
                     </div>
                     <div class="col-md-6">
                        <div class="form-input mt-15">
                           <label>Operador</label>
                           <div class="input-items default">
                              
                              <select class="form-select" name="operador" required>
                                    <option value="No">No</option>
                                    <option value="Si">Si</option>
                                </select>
                           </div>
                        </div>
                        <!-- form input -->
                     </div>

                     <div class="col-md-6">
                        <div class="form-input mt-15">
                           <label>Región</label>
                           <div class="input-items default">
                              
                              <select class="form-select" name="region" id="regiones" aria-label="Default select example" onchange="ciudad(this.id, 'ciudades')" required>
                                    {{#each regiones}}
                                    <option value="{{name}}">{{name}}</option>
                                    {{/each}}
                                </select>
                           </div>
                        </div>
                        <!-- form input -->
                     </div>

                      <div class="col-md-6">
                        <div class="form-input mt-15">
                           <label>Ciudades</label>
                           <div class="input-items default">
                              
                              <select class="form-select" name="ciudades"  id="ciudades" aria-label="Default select example" required>
                            {{#each ciudades}}
                            <option value="{{name}}">{{name}}</option>
                            {{/each}}
                        </select>
                           </div>
                        </div>
                        <!-- form input -->
                     </div>

                     <div class="col-md-6">
                        <div class="form-input mt-15">
                           <label>Estado del Servicio</label>
                           <div class="input-items default">
                              <select id="inputState" name="estado" class="form-select" required>
                                <option value="Disponible">Disponible</option>
                                <option value="En mantenimiento">En mantenimiento</option>
                                <option value="Ocupada">Ocupada</option>
                                </select>
                           </div>
                        </div>
                        <!-- form input -->
                     </div>

                     <div class="col-md-6">
                        <div class="form-input mt-15">
                           <label>Categoría</label>
                           <div class="input-items default">
                              <i class="lni lni-envelope"></i>
                              <input type="text" placeholder="Retroexcavadora" name="categoria" id="inputZip" required />
                           </div>
                        </div>
                        <!-- form input -->
                     </div>

                     <div class="col-md-12">
                        <div class="form-input mt-15">
                           <label>Describa su servicio</label>
                           <div class="input-items default">
                              <i class="lni lni-pencil-alt"></i>
                              <textarea
                                    name="description"
                                 placeholder="Cuentanos sobre el servicio"
                                 ></textarea>
                           </div>
                        </div>                         
                        <!-- form input -->
                     </div>

                     <div class="col-md-6">
                        <div class="form-input mt-15">
                           <label>Dominio de la máquina</label>
                           <div class="input-items default">
                              <i class="lni lni-files"></i>
                              <input id="domMaq" name="domMaq" type="file"/>
                           </div>
                        </div>
                        <!-- form input -->
                     </div>


                    <div class="col-md-6">
                        <div class="form-input mt-15">
                           <label>Revisión técnica</label>
                           <div class="input-items default">
                              <i class="lni lni-files"></i>
                              <input id="revTec" name="revTec" type="file"/>
                           </div>
                        </div>
                        <!-- form input -->
                     </div>

                     <div class="col-md-6">
                        <div class="form-input mt-15">
                           <label>Permiso de circulación</label>
                           <div class="input-items default">
                              <i class="lni lni-files"></i>
                              <input id="perCir" name="perCir" type="file"/>
                           </div>
                        </div>
                        <!-- form input -->
                     </div>

                     <div class="col-md-6">
                        <div class="form-input mt-15">
                           <label>Seguro</label>
                           <div class="input-items default">
                              <i class="lni lni-files"></i>
                              <input id="seg" name="seg"type="file"/>
                           </div>
                        </div>
                        <!-- form input -->
                     </div>

                     <div class="col-md-6">
                        <div class="form-input mt-15">
                           <label>Documentación del operador (si aplica)</label>
                           <div class="input-items default">
                              <i class="lni lni-files"></i>
                              <input id="docOpe" name="docOpe" type="file"/>
                           </div>
                        </div>
                        <!-- form input -->
                     </div>

                    


                     <div class="col-md-12">
                        <div class="single-form mt-15">
                           <div class="input-form rounded-buttons">
                              <button
                                 class="btn primary-btn rounded-full"
                                 type="submit"
                                 >
                              Publicar Solicitud
                              </button>
                           </div>
                        </div>
                        <!-- single form -->
                     </div>
                  </div>
                  <!-- row -->
               </form>
            </div>
      <!-- row -->
   </div>
   <!-- container -->
</section>
<!--====== CONTACT ONE PART ENDS ======-->
</div>


